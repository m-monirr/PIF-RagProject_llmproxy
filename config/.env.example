# Qdrant Configuration
QDRANT_HOST=localhost
QDRANT_PORT=6333

# Model Configuration
EMBEDDING_MODEL=BAAI/bge-m3
EMBEDDING_DEVICE=cpu

# API Configuration
API_HOST=0.0.0.0
API_PORT=8080

# Optional: OpenAI API Key (if using GPT models)
OPENAI_API_KEY=sk-your-openai-api-key-here

# Optional: Hugging Face Token (for private models)
# HF_TOKEN=your_token_here

# Ollama Cloud Configuration
# Get your API key from https://ollama.ai/cloud

# For Ollama Cloud (remote):
OLLAMA_BASE_URL=https://api.ollama.ai
OLLAMA_API_KEY=your_api_key_here

# For Local Ollama (uncomment to use local):
# OLLAMA_BASE_URL=http://localhost:11434
# OLLAMA_API_KEY=

# Alternative: Cohere Configuration
# COHERE_API_KEY=your-cohere-api-key-here

# Groq Configuration (Free Tier - Very Fast!)
# Get your free API key from https://console.groq.com/keys
GROQ_API_KEY=your-groq-api-key-here